    if(my_dancer != undefined){
        if(instance_place(x, y, obj_player) == my_dancer){
            if(!ritual_happening){
                random_dance_ritual = ceil(random(current_time) % 3)
                ritual_happening = true
            }
            
            if(random_dance_ritual == my_dancer.dancing){
                health -= (1 / room_speed)
                if (health &lt; 0) health = 0
                image_index = maxHealth - floor(health);
